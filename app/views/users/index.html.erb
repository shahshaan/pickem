<ul>
  <% @users.each do |user| %>
    <li>
      (<%= user.id %>) <%= link_to user.full_name, user_path(user.id) %>
    </li>
  <% end -%>
</ul>


<% Matchup.all_finished.each do |matchup| %>
  <%= matchup.display %>
  <%= matchup.winner.name %>
  <%# current_user.picks.where(matchup_id: matchup.id).each do |pick| %>
    <p><%#= pick.team.name %></p>
  <%# end %>
<% end %>

<%#= current_user.score %>
<div class="row">
  <div class="col-md-2"></div>
  <div class="col-md-8">
    <table class="table">
      <thead>
        <tr>
          <th>Rank</th>
          <th>Name</th>
          <th>Points</th>
          <th>Percentage</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td>#</td>
            <td><%= user.full_name %></td>
            <td><%= user.score %></td>
            <td><%= user.pick_percentage %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
  <div class="col-md-2"></div> 
</div>
